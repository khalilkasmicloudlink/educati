			$urlMid .= JTHZ;			$urlMid = str_replace("-/","/",$urlMid);			$urlMid = str_replace("-".JTHZ,JTHZ,$urlMid);		
		
			
			while(strstr($urlMid,'--'))
				$urlMid = str_replace('--','-',$urlMid);	
			// $urlMid = str_replace('/-','/',$urlMid);	
			
		$rtStr = $filePres.$urlMid;
		$rtStr = str_replace($filePres.'-',$filePres,$rtStr);
		$rtStr = str_replace('-amp;-','-&amp;-',$rtStr);
		return $rtStr;
	}
	

  
  
  
	
	
	
